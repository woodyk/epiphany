    old_set =3D set([entry["message"] for entry in old_data])
    new_set =3D set([entry["message"] for entry in new_data])

    additions =3D new_set - old_set
    deletions =3D old_set - new_set

    return {"additions": additions, "deletions": deletions}

def main():
    parser =3D argparse.ArgumentParser(description=3D"Compare two JSONL fil=
es.")
    parser.add_argument("--old", "-o", required=3DTrue, help=3D"Old JSONL f=
ile")
    parser.add_argument("--new", "-n", required=3DTrue, help=3D"New JSONL f=
ile")
    args =3D parser.parse_args()

    old_data =3D load_jsonl_file(args.old)
    new_data =3D load_jsonl_file(args.new)

    changes =3D find_changes(old_data, new_data)

    print("\n=3D=3D=3D Changes =3D=3D=3D\n")
    print(f"\nAdded:\n{'-\n'.join(sorted(changes['additions']))}\n")
    print(f"\nDeleted:\n{'-\n'.join(sorted(changes['deletions']))}\n")

